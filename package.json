  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed --workers=1",
    "e2e:debug": "playwright test --debug",
    "e2e:file": "playwright test __tests__/automation/people.spec.ts --headed",
    "e2e:report": "playwright test --reporter=html",
    "e2e:show-report": "playwright show-report"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
