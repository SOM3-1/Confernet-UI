    "@mui/material": "^6.4.7",
    "@mui/x-date-pickers": "^7.28.0",
    "axios": "^1.8.4",
    "date-fns": "^2.30.0",
    "firebase": "^11.4.0",
    "playwright": "^1.51.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-firebase-hooks": "^5.1.1",
    "react-qr-code": "^2.0.15",
    "react-router-dom": "^7.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
